class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        if(!head){
            return NULL;
        }
        ListNode* curr=head;
        ListNode* dum=NULL;
        while(curr){
            ListNode* temp=curr->next;
            curr->next=dum;
            dum=curr;
            curr=temp;
        }
        return dum;
    }
};
